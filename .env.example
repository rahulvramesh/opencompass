# OpenCompass Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key
OPENAI_API_KEY=

# Anthropic Claude API Key
ANTHROPIC_API_KEY=

# Zhipu AI API Key
ZHIPUAI_API_KEY=

# Alibaba DashScope API Key
DASHSCOPE_API_KEY=

# Baidu API Key
BAIDU_API_KEY=

# Moonshot API Key
MOONSHOT_API_KEY=

# Other API Keys
MINIMAX_API_KEY=
XUNFEI_API_KEY=
SENSETIME_API_KEY=

# Model Cache Settings
HF_HOME=/workspace/cache
TRANSFORMERS_CACHE=/workspace/cache
TORCH_HOME=/workspace/cache

# Jupyter Settings
JUPYTER_PASSWORD=opencompass

# GPU Settings (for docker-compose)
# NVIDIA_VISIBLE_DEVICES=all
# CUDA_VISIBLE_DEVICES=0,1

# Memory Settings
# PYTORCH_CUDA_ALLOC_CONF=max_split_size_mb:512